{
    "workflow": {
        "settings": {
            "cpu_enable": false,
            "gpu_mask": "11",
            "log": "../storage/scuola/progetto_scuola/"
        },
        "project":{
            "path": "../storage/scuola/progetto_scuola"
        },
        "PhotoProcessor":{
            "matchPhotos":{
                "downscale": 1,
                "keypoint_limit": 40000,
                "tiepoint_limit": 10000,
                "generic_preselection": true,
                "reference_preselection": false,
                "filter_stationary_points": true,
                "keep_keypoints": true,
                "guided_matching": false,
                "subdivide_task": true
            },
            "alignCameras":{
                "adaptive_fitting": false,
                "reset_alignment": true,
                "subdivide_task": true
            },
            "optimizeCameras":{
                "fit_f": true, 
                "fit_cx": true, 
                "fit_cy": true, 
                "fit_b1": false, 
                "fit_b2": false, 
                "fit_k1": true,
                "fit_k2": true, 
                "fit_k3": true, 
                "fit_k4": false, 
                "fit_p1": true, 
                "fit_p2": true, 
                "fit_corrections": false,
                "adaptive_fitting": false, 
                "tiepoint_covariance": false
            }
        },
        "PointCloudProcessor":{
            "buildDepthMaps":{
                "downscale": 2,
                "filter_mode": "Metashape.FilterMode.MildFiltering",
                "reuse_depth": false,
                "subdivide_task": true
            },
            "buildPointCloud":{
                "source_data": "Metashape.DataSource.DepthMapsData",
                "point_colors": true,
                "point_confidence": true,
                "keep_depth": true,
                "subdivide_task": true,
                "maxconf": 3
            },
            "colorizePointCloud":{
                "source_data": "Metashape.ImagesData",
                "subdivide_task": true
            }
        },
        "3DModelProcessor":{
            "buildModel":{
                "surface_type": "Metashape.Arbitrary",
                "source_data": "Metashape.DepthMapsData",
                "interpolation": "Metashape.EnabledInterpolation",
                "face_count": "Metashape.HighFaceCount",
                "vertex_colors": true,
                "vertex_confidence": true,
                "keep_depth": true,
                "split_in_blocks": false,
                "blocks_size": 250,
                "build_texture": true,
                "subdivide_task": true
            },
            "buildTexture":{
                "blending_mode": "Metashape.MosaicBlending",
                "texture_size": 8192,
                "fill_holes": true,
                "ghosting_filter": true
            },
            "buildUV":{
                "mapping_mode": "Metashape.GenericMapping",
                "page_count": 1,
                "texture_size": 8192
            },
            "buildTiledModel":{
                "pixel_size": 0,
                "tile_size": 256,
                "source_data": "Metashape.DataSource.DepthMapsData",
                "face_count": 20000,
                "ghosting_filter": false,
                "transfer_texture": false,
                "keep_depth": true,
                "subdivide_task": true
            },
            "exportTexture": {}
        },
        "OrthoAndDEMCreation":{
            "buildDem":{
                "source_data": "Metashape.PointCloudData",
                "interpolation": "Metashape.EnabledInterpolation",
                "subdivide_task": true
            },
            "buildOrtho":{
                "surface_data": "Metashape.ElevationData",
                "fill_holes": true,
                "subdivide_task": true,
                "blending_mode": "Metashape.BlendingMode.MosaicBlending"
            }
        }

    }
}